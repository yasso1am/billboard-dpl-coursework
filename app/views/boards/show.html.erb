<h1> Board: <%= @board.name %> </h1>
<%= link_to 'Add a song to the board!', new_board_song_path(@board) %>
<hr />
<h3> Artists </h3>
  <ul>
  <% @board.artists.each do |artist| %>
  <li>
    <%= link_to artist.name, artist_path(artist) %>
    </li>
    <% end %>
  </ul>
<hr />
<button> <%= link_to 'Edit Board Name', edit_board_path(@board) %> </button>
<button> <%= link_to 'Delete Board', @board, method: :delete %> </button>
<button> <%= link_to 'Back', boards_path %> </button>